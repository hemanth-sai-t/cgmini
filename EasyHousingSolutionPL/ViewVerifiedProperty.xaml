<Window x:Class="EasyHousingSolutionPL.ViewVerifiedProperty"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasyHousingSolutionPL"
        mc:Ignorable="d"
        Title="ViewVerifiedProperty" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid  Background="{StaticResource Image}">
        <DataGrid Name="dgViewProperties" ItemsSource="{Binding}" HorizontalAlignment="Left" Height="204" Margin="145,110,0,0" VerticalAlignment="Top" Width="507"/>
        <TextBlock HorizontalAlignment="Left" Margin="652,21,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="113">
            <Hyperlink NavigateUri="Login.xaml" Click="Hyperlink_Click">
                Logout
            </Hyperlink>
        </TextBlock>
        <TextBlock HorizontalAlignment="Left" Margin="320,40,0,0" TextWrapping="Wrap" Text="Verified Properties" VerticalAlignment="Top" Width="203" FontSize="22" FontWeight="Bold"/>
    </Grid>
</Window>